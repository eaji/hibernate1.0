create table PERSON (id int not null auto_increment, first_name varchar(255), middle_name varchar(255), last_name varchar(255), address int not null, birthdate date, gwa float,  employed bit, primary key (id) );

create table ADDRESS (id int not null auto_increment, street_number varchar(255), barangay varchar(255), city varchar(255), zip_code varchar(255), primary key (id));

create table CONTACT (id int not null auto_increment, contact_type int, value varchar(255), person_id int not null, primary key (id));

